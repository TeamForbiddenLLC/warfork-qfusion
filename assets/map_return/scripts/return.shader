textures/return/dragon_decal
{
	qer_editorimage textures/return/dragon_decal.png
	surfaceparm nomarks
	surfaceparm nonsolid
	surfaceparm trans
	qer_alphaFunc gequal 0.5
	polygonoffset

	{
		map textures/return/dragon_decal.png
		alphaFunc GE128
		depthWrite
	}

	{
		map $lightmap
		blendFunc filter
		depthFunc equal
	}
}

