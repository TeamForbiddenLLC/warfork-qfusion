msgid "bindlist"
msgstr "Lists all bound keys."  

#. Console/prefixless
msgid "botdebug"
msgstr "Bot debug mode.\n"
"^6Note: You must also enable sv_cheats \"1\" for this command to work."

#. Console/prefixless
msgid "botnotarget"
msgstr "Bots will not target you.\n"
"^6Note: You must also enable sv_cheats \"1\" for this command to work. practice moving bots."

#. Console/prefixless
msgid "callvote"
msgstr "<vote> [argument] Calls a vote."

#. Console/prefixless
msgid "camswitch"
msgstr "Switches between player chasing and free look when in spectator mode."

#. Console/prefixless
msgid "centerview"
msgstr "Centers the players view."

#. Console/prefixless
msgid "chase"
msgstr "Joins spectator mode and chases players."

#. Console/prefixless
msgid "chasenext"
msgstr "Chases the next player in spectator mode.\n"
"^6Note: You must also be using \"chase\" mode."

#. Console/prefixless
msgid "chaseprev"
msgstr "Chases the previous player in spectator mode.\n"
"^6Note: You must also be using \"chase\" mode."
 
#. Console/prefixless
msgid "cinematic"
msgstr "<name> [flags] Plays specified cinematic."
 
#. Console/prefixless
msgid "cinepause"
msgstr "Pauses cinematic (if playing)."

#. Console/prefixless
msgid "cinlist"
msgstr "Lists all active cinematics."

#. Console/prefixless
msgid "clear"
msgstr "Clears the console."

#. Console/prefixless
msgid "clearcams"
msgstr "Clears all cameras in a camerapath."

#. Console/prefixless
msgid "cmd"
msgstr "<commmand> Runs any argument(s) as if it were a command."

#. Console/prefixless
msgid "cmdlist"
msgstr "Lists all console commands."

#. Console/prefixless
msgid "coach"
msgstr "Allows you to be a coach in some team based gametypes. Be on the team you wish to coach before typing the command in console."  

#. Console/prefixless
msgid "cointoss"
msgstr "<heads/tails> Flips a coin based on your choice and announces if you won or lost."  

#. Console/prefixless
msgid "condump"
msgstr "<filename> Dumps the console log to a file."  

#. Console/prefixless
msgid "connect"
msgstr "<ip address> Connects to a server."  

#. Console/prefixless
msgid "cvarcheck"
msgstr "<userid> <cvar name> This server command reports the defined player's value for the defined cvar.\n"
"^6Example: rcon cvarcheck 0 name\n"
"^6Note: Only works from a server context, not client.\n"
"^6This command is currently not working as intended."

#. Console/prefixless
msgid "cvarinfo"
msgstr "<cvar name> This command reports the current value for the defined cvar.\n"
"^6Note: Only works from a client or gametype context, not server.\n"
"^6This command is currently not working as intended."

#. Console/prefixless
msgid "cvarlist"
msgstr "Lists all cvars."

#. Console/prefixless
msgid "deleteclosestnode"
msgstr "Removes the closest bot navigation node to your current location."

#. Console/prefixless
msgid "deletecam"
msgstr "Deletes the current camera.\n"
"^6Note: Only works in \"demoEditMode\"."

#. Console/prefixless
msgid "demo"
msgstr "<filename> Loads a specific demo."

#. Console/prefixless
msgid "demoavi"
msgstr "Starts recording video as frames or audio as .wav file depending on cl_demoavi_audio and cl_demoavi_video. Framerate depends on cl_demoavi_fps."

#. Console/prefixless
msgid "demoEditMode"
msgstr "Lets you add and edit cameras for camera paths. Displays additional information on cameras (e.g.: timecode, current camera, next camera, type, position, roll, pitch, yaw, fov)."

#. Console/prefixless
msgid "demoFreeFly"
msgstr "Uses free flying camera regardless of set camera paths.\n"
"^6Note: Entities like players may disappear on single POV demos when camera moves too far from the player."

#. Console/prefixless
msgid "demoget"
msgstr "<ID> Use with demolist to download recorded demos from a server." 

#. Console/prefixless
msgid "demojump"
msgstr "Jumps to a specified time in a demo. Time format is [minutes:]seconds. Use + or - in front of the time to specify it in relation to current position in the demo."

#. Console/prefixless
msgid "demolist"
msgstr "Lists all demos on the Server and their ID.\n"
"^6Note: Can be downloaded via demoget <ID>"

#. Console/prefixless
msgid "demopause"
msgstr "Pauses or resumes the currently playing demo."

#. Console/prefixless
msgid "devmap"
msgstr "<map> Restarts the game, enables cheats, and starts the map.\n"
"^6Note: Other available options are \"map\" and \"gamemap\"."

#. Console/prefixless
msgid "disc"
msgstr "Disconnects from current server."

#. Console/prefixless
msgid "disconnect"
msgstr "Disconnects from current server."

#. Console/prefixless
msgid "downloadcancel"
msgstr "Cancels an active download."

#. Console/prefixless
msgid "downloadstatus"
msgstr "Displays download status information."  

#. Console/prefixless
msgid "dumpASapi"
msgstr "Dump all Angelscript API variables, classes, and global functions into files.\n"
"^6Note: It will dumped to \"basewf/AS_API/\" within your data folder. The location depends based on your Operating System."

#. Console/prefixless
msgid "dumpuser"
msgstr "<userid> Prints a specific player's user info settings.\n"
"^6Examples: cg_movementStyle, cg_noAutohop, cl_mm_session, clan, color, hand, handicap, model, name, password, and skin."

#. Console/prefixless
msgid "dynvarlist"
msgstr "Lists dynamic variables.\n"
"^6Note: Dynamic variables differ from regular variables in that when you request or set their values, it activates a function associated with them."

#. Console/prefixless
msgid "echo"
msgstr "<message> Prints a message to the console."

#. Console/prefixless
msgid "editcam"
msgstr "<command> <variable> Edits the current camera.\n"
"^6Note: You can edit: type <type name> eg. path_spline; track <entity number> (tracks entity; 0 for no track); fov; timeOffset; origin; angles; pitch; yaw; roll."

#. Console/prefixless
msgid "editnodes"
msgstr "Modifies the currently loaded bot navigation nodes in your map, without deleting the existing file."

#. Console/prefixless
msgid "enterqueue"
msgstr "Enters you into the challengers queue.\n"
"^6Note: The gametype must support a queue, for example \"duel\"."

#. Console/prefixless
msgid "envshot"
msgstr "Creates an environment map.\n"
"^6Note: It will dumped to \"basewf/env/\" within your data folder as \"px\" \"nx\" \"py\" \"ny\" \"pz\" and \"nz\" in .tga format. The location depends based on your Operating System."

#. Console/prefixless
msgid "exec"
msgstr "<filename> Executes a configuration file."

#. Console/prefixless
msgid "fontlist"
msgstr "Prints a font list."

#. Console/prefixless
msgid "gamemap"
msgstr "<map> Starts the map.\n"
"^6Note: Other available options are \"devmap\" and \"map\"."

#. Console/prefixless
msgid "gamemenu"
msgstr "Launches the in-game menu when connected to a server.\n"
"^6Note: By default you can also launch this menu by pressing the \"Escape\" key."

#. Console/prefixless
msgid "gametype"
msgstr "Lists information about current gametype."

#. Console/prefixless
msgid "getinfo"
msgstr "Used for populating server browser lists and providing players with a quick overview of available game servers."

#. Console/prefixless
msgid "getstatus"
msgstr "Used to send a query to a game server to retrieve detailed status information."

#. Console/prefixless
msgid "gfxinfo"
msgstr "Prints graphics-related information, such as hardware details, rendering capabilities, texture settings, and screen resolution."

#. Console/prefixless
msgid "give"
msgstr "<value> Give items to a Player.\n"
"^6Everything: give \"all\"\n"
"^6Health: give \"health\"\n"
"^6Weapons: give \"weapons\"\n"
"^6Ammo: give \"ammo\"\n"
"^6Armor: give \"armor\""

#. Console/prefixless
msgid "glslprogramlist"
msgstr "Lists and displays details of GLSL shader programs, including their names and features, providing insight into the shaders used for graphics rendering."

#. Console/prefixless
msgid "god"
msgstr "Toggles godmode."

#. Console/prefixless
msgid "heartbeat"
msgstr "Sends a manual heartbeat to the masterservers.\n"
"^6Note: This makes your server visible to those that query the list of servers."

#. Console/prefixless
msgid "help_hud"
msgstr "Prints a list of HUD scripts commands, HUD scripts operators, HUD scripts CONSTANT names, and HUD scripts REFERENCE names to your console."

#. Console/prefixless
msgid "imagelist"
msgstr "Shows a list of all images loaded in your console."

#. Console/prefixless
msgid "importcams"
msgstr "<filename> imports a .cam file.\n"
"^6Note: Filepath is relative to the demos directory. The cameras imported are merged with the previously existing, if any."

#. Console/prefixless
msgid "invite"
msgstr "<player> Invite a player to your team."

#. Console/prefixless
msgid "join"
msgstr "[team name] Joins the game. Joins a specific team if specified."

#. Console/prefixless
msgid "kick"
msgstr "<player> Kicks a player from the server."

#. Console/prefixless
msgid "kill"
msgstr "Kills the player."

#. Console/prefixless
msgid "killserver"
msgstr "Kills the server process."

#. Console/prefixless
msgid "leavequeue"
msgstr "Leaves the challengers queue."

#. Console/prefixless
