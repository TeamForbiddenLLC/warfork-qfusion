#. Console/prefixless
msgid "autohelp"
msgstr "<0/1> Prints descriptions for console variables in console.\n"
"^6Disable: autohelp \"0\"\n"
"^6Enable: autohelp \"1\"\n"
"^6Note: Prints after execution."

#. Console/prefixless
msgid "clan"
msgstr "<name> Sets the name of your clan.\n"
"^6Note: Appears on the scoreboard."

#. Console/prefixless
msgid "color"
msgstr "<R G B> Sets your Player color.\n"
"^6Red: color \"255 0 0\"\n"
"^6Blue: color \"51 128 255\"\n"
"^6Green: color \"0 204 102\"\n"
"^6Orange: color \"255 128 0\"\n"
"^6Yellow: color \"255 255 51\"\n"
"^6Purple: color \"153 51 255\"\n"
"^6Note: The list above only includes RGB color examples and is not all inclusive."

#. Console/prefixless
msgid "debuggraph"
msgstr "<0/1> The debug graph will be displayed.\n"
"^6Disable: debuggraph \"0\"\n"
"^6Enable: debuggraph \"1\"\n"
"^6Note: Represents various values, such as dropped network packets and latency."

#. Console/prefixless
msgid "dedicated"
msgstr "Specifies if the server is \"dedicated\".\n"
"^6Disable: dedicated \"0\"\n"
"^6Enable: dedicated \"1\"\n"
"^6Note: Non-dedicated servers are running the Warfork game client, while also hosting a server for others to join at the same time. This typically results in lesser performance."

#. Console/prefixless
msgid "developer"
msgstr "<0/1> Developer mode.\n"
"^6Disable: developer \"0\"\n"
"^6Enable: developer \"1\"\n"
"^6Note: Developer mode only features and console prints will be enabled."

#. Console/prefixless
msgid "developerMemory"
msgstr "<0/1> Prints memory allocation information to console.\n"
"^6Disable: developerMemory \"0\"\n"
"^6Enable: developerMemory \"1\"\n"
"^6Note: developer \"1\" must be set for this command to work."

#. Console/prefixless
msgid "favorites"
msgstr "Amount of favorite servers."

#. Console/prefixless
msgid "filterban"
msgstr "<0/1> Filters connection attempts based on IP addresses.\n"
"^6IP addresses matching the current list will be disallowed from entering the game: filterban \"0\"\n"
"^6Only addresses that match the list will be granted access: filterban \"1\"\n"
"^6Note: Setting it to \"1\" is recommended for easily configuring a private game or allowing only local network players."

#. Console/prefixless
msgid "fixedtime"
msgstr "<value> Sets gamemsec to the defined integer to have the game run at the defined speed.\n"
"^6Note: You likely only want to set this if you are making a video from demos.\n"
"^6If neither fixedtime nor timescale are set, gamemsec is equal to realmsec (real time)\n"
"^6sv_cheats \"1\" is required to use this command."

#. Console/prefixless
msgid "fov"
msgstr "<value> Changes your Field of view (fov)."

#. Console/prefixless
msgid "gamedate"
msgstr "<value> The date that the Warfork binary was built.\n"
"^6Note: The format will be mmm dd yyyy (Example: \"Jan 1 2023\")"

#. Console/prefixless
msgid "gamename"
msgstr "<value> Name of the game."

#. Console/prefixless
msgid "graphheight"
msgstr "<value> Changes the graph height (timegraph, netgraph, etc.)."

#. Console/prefixless
msgid "graphscale"
msgstr "<value> Scales the graph (timegraph, netgraph, etc.) for resolutions with a width greather than 1024."

#. Console/prefixless
msgid "graphshift"
msgstr "<value> Vertically shifts the graph (timegraph, netgraph, etc.) for readability."

#. Console/prefixless
msgid "hand"
msgstr "<0/1> Changes weapon handedness.\n"
"^6Right: hand \"0\"\n"
"^6Left: hand \"1\""

#. Console/prefixless
msgid "handicap"
msgstr "<0-90> Decreases your damage output by defined percentage value.\n"
"^6Note: This is typically used for dueling players below your skill level."

#. Console/prefixless
msgid "lang"
msgstr "<language> Sets the language of Warfork.\n"
"^6Belarusian: lang \"be\"\n"
"^6Bulgarian: lang \"bg\"\n"
"^6Chinese (China): lang \"zh_CN\"\n"
"^6Dutch: lang \"nl\"\n"
"^6English: lang \"en\"\n"
"^6Finnish: lang \"fi\"\n"
"^6French: lang \"fr\"\n"
"^6German: lang \"de\"\n"
"^6Hungarian: lang \"hu\"\n"
"^6Italian: lang \"it\"\n"
"^6Japanese: lang \"ja\"\n"
"^6Lithuanian: lang \"lt\"\n"
"^6Polish: lang \"pl\"\n"
"^6Portuguese: lang \"pt\"\n"
"^6Portuguese (Brazil): lang \"pt_BR\"\n"
"^6Romanian: lang \"ro\"\n"
"^6Russian: lang \"ru\"\n"
"^6Spanish: lang \"es\"\n"
"^6Spanish (Latin America): lang \"es_419\"\n"
"^6Swedish: lang \"sv\"\n"
"^6Turkish: lang \"tr\"\n"
"^6Ukranian: lang \"uk\"\n"
"^6Vietnamese: lang \"vn\"\n"
"^6Note: You can execute the command \"vid_restart\" after setting the language for it to change in-game. The list above only includes language examples and is not all inclusive."

#. Console/prefixless
msgid "lookspring"
msgstr "<0/1> The screen will be automatically centered when using +mlook.\n"
"^6Disable: lookspring \"0\"\n"
"^6Enable: lookspring \"1\""

#. Console/prefixless
msgid "lookstrafe"
msgstr "<0/1> Automatic strafing will be toggled when using +mlook.\n"
"^6Disable: lookstrafe \"0\"\n"
"^6Enable: lookstrafe \"1\""

#. Console/prefixless
msgid "mapname"
msgstr "<value> The name of current map played."

#. Console/prefixless
msgid "masterservers"
msgstr "<value> Specifies the master servers used to query a list of Warfork servers."

#. Console/prefixless
msgid "masterservers_warfork"
msgstr "<value> Specifies the (warfork-specific) master servers used to query a list of Warfork servers."

#. Console/prefixless
msgid "model"
msgstr "<model> Changes your Player model."

#. Console/prefixless
msgid "name"
msgstr "<name> Changes your nickname."

#. Console/prefixless
msgid "netgraph"
msgstr "<0/1> Information will be displayed on the bottom of the screen about all network packets.\n"
"^6Disable: netgraph \"0\"\n"
"^6Enable: netgraph \"1\""

#. Console/prefixless
msgid "nextmap"
msgstr "<value> Prints the next map.\n"
"^6Note: You can force the next map with \"match advance\"."

#. Console/prefixless
msgid "password"
msgstr "<password> If a server requires a password, the client automatically tries to join with the specific password."

#. Console/prefixless
msgid "logconsole"
msgstr "<value> Automatically logs console to a specified file when defined.\n"
"^6Note: Operating System Path\n"
"^6Windows %USERPROFILE%/My Documents/My Games/Warfork version.number/basewf/\n"
"^6Mac ~/Library/Application Support/Warfork-version.number/basewf/\n"
"^6Linux ~/.local/share/warfork-version.number/basewf/\n"
"^6Note: If fs_usehomedir \"0\" is set, then your log file will be stored in your/warfork/game/directory/basewf/."

#. Console/prefixless
msgid "protocol"
msgstr "<value> Specifies the Application Protocol Version.\n"
"^6Note: The protocol can be changed by modifying \"APP_PROTOCOL_VERSION\" found in \"source/qcommon/version.h\" and recompiling Warfork."

#. Console/prefixless
msgid "sensitivity"
msgstr "<value> Changes sensitivity of your mouse."

#. Console/prefixless
msgid "showdrop"
msgstr "<0/1> Information will be displayed in the console about dropped network packets.\n"
"^6Disable: showdrop \"0\"\n"
"^6Enable: showdrop \"1\""

#. Console/prefixless
msgid "showpackets"
msgstr "<0/1> Information will be displayed in the console about all network packets.\n"
"^6Disable: showpackets \"0\"\n"
"^6Enable: showpackets \"1\""

#. Console/prefixless
msgid "skin"
msgstr "<skin> Changes your Player skin."

#. Console/prefixless
msgid "timedemo"
msgstr "<0/1> Sets whether or not a demo should be timed when played.\n"
"^6Disable: timedemo \"0\"\n"
"^6Enable: timedemo \"1\"\n"
"^6Note: Once set to \"1\", play a demo and then check console. Console will show stats related to the length and playback speed of the demo."

#. Console/prefixless
msgid "timegraph"
msgstr "<0/1> A graph will be displayed, which visualizes the compression and expansion of game time.\n"
"^6Disable: timegraph \"0\"\n"
"^6Enable: timegraph \"1\"\n"
"^6Note: This is necessary to maintain the overall game time constant."

#. Console/prefixless
msgid "timescale"
msgstr "<value> Changes the scale that game time advances in relation to real time.\n"
"^6Note: sv_cheats \"1\" is required to use this command."

#. Console/prefixless
msgid "version"
msgstr "<value> The version of the game.\n"
"^6Example: 2.12 x86_64 May 17 2023 Linux"

#. Console/prefixless
msgid "zoomfov"
msgstr "<3-75> Changes the field of view while zooming."

#. Console/prefixless
msgid "zoomsens"
msgstr "<value> Changes your sensitivity while zooming.\n"
"^6Note: zoomsens \"0\" uses your normal sensitivity."

